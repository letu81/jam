<%= form_tag(update_version_path('city'), {class: 'form-horizontal'}) do %>
    <fieldset class="inputs">
      <legend><span>城市管理</span></legend>
      <div class="form-group form-group-sm">
        <label class="col-sm-2 control-label" for="service_interests">已开通</label>

        <div class="col-sm-10">
          <%= text_field_tag('opened', city['opened'].join(','), class: 'form-control', placeholder: '请输入最新到版本号') %>
        </div>
      </div>
      <div class="form-group form-group-sm">
        <label class="col-sm-2 control-label" for="service_interests">正在开通</label>

        <div class="col-sm-10">
          <%= text_field_tag('opening', city['opening'].join(','), class: 'form-control', placeholder: '请输入最新到版本号') %>
        </div>
      </div>
    </fieldset>
    <%= submit_tag('提交') %>
<% end %>